{
  "name": "ui-prototype",
  "version": "0.0.0",
  "description": "A clean, fully keyboard-enabled, feature-rich UI prototype designed specifically for Electron.",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron .",
    "compile-ts": "tsc && tsc-alias",
    "compile-style": "node tools/compiler/style",
    "compile-locale": "node tools/compiler/locale",
    "compile-assets": "node tools/compiler/assets",
    "compile-pug": "node tools/compiler/view",
    "compile": "node tools/compiler/all && npm run compile-ts",
    "test": "jest",
    "lint": "prettier -w --config .prettierrc ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonnyJong/UI-PROTOTYPE.git"
  },
  "keywords": [
    "electron",
    "ui"
  ],
  "author": "jonnyjong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JonnyJong/UI-PROTOTYPE/issues"
  },
  "homepage": "https://github.com/JonnyJong/UI-PROTOTYPE#readme",
  "devDependencies": {
    "@types/color": "^3.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@types/pug": "^2.0.10",
    "@types/stylus": "^0.48.42",
    "electron": "^30.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rfdc": "^1.3.1",
    "stylus": "^0.63.0",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.4.5",
    "yaml": "^2.4.1"
  },
  "dependencies": {
    "color": "^4.2.3",
    "io-ts": "^2.2.21",
    "pug": "^3.0.2",
    "wallpaper": "^7.2.1"
  }
}
